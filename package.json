{
  "name": "tools-ui",
  "version": "0.0.1",
  "description": "Single page app for `tools-api`",
  "main": "",
  "scripts": {
    "start": "node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "serve:stage": "node server.js --env=stage",
    "dist": "npm run copy & webpack --env=dist",
    "stage": "npm run copy & webpack --env=stage",
    "lint": "eslint ./src",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "clean": "rimraf dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NojYerac/tools-ui.git"
  },
